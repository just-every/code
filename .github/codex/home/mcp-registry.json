    "repo-search": {
      "type": "stdio",
      "command": "/usr/bin/zsh",
      "args": [
        "-c",
        "/home/thetu/.local/bin/uvx awslabs.git-repo-research-mcp-server@latest"
      ]
    },
    "doc-index": {
      "type": "stdio",
      "command": "/usr/bin/zsh",
      "args": [
        "-c",
        "/home/thetu/.npm-global/bin/npx -y open-docs-mcp --docsDir /home/thetu/code/docs"
      ]
    },
    "shell-lite": {
      "type": "stdio",
      "command": "/usr/bin/zsh",
      "args": [
        "-c",
        "/home/thetu/.npm-global/bin/npx -y super-shell-mcp"
      ]
    },
    "git-status": {
      "type": "stdio",
      "command": "/usr/bin/zsh",
      "args": [
        "-c",
        "/home/thetu/.local/bin/uvx mcp-server-git --repository /home/thetu/code"
      ]
    }
  },
  "aliases": {}
