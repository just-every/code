[
  {
    "name": "git_repo_research",
    "command": "uvx",
    "args": [
      "awslabs.git-repo-research-mcp-server@latest"
    ],
    "env": null,
    "startup_timeout_ms": null
  },
  {
    "name": "open_docs",
    "command": "npx",
    "args": [
      "-y",
      "open-docs-mcp",
      "--docsDir",
      "/home/thetu/code/docs"
    ],
    "env": null,
    "startup_timeout_ms": null
  },
  {
    "name": "registry",
    "command": "npx",
    "args": [
      "-y",
      "mcp-proxy",
      "https://registry.modelcontextprotocol.io/api/sse"
    ],
    "env": null,
    "startup_timeout_ms": null
  },
  {
    "name": "server_git",
    "command": "uvx",
    "args": [
      "mcp-server-git",
      "--repository",
      "/home/thetu/code"
    ],
    "env": {
      "GIT_PAGER": "cat"
    },
    "startup_timeout_ms": null
  },
  {
    "name": "super_shell",
    "command": "npx",
    "args": [
      "-y",
      "super-shell-mcp"
    ],
    "env": null,
    "startup_timeout_ms": null
  }
]
